      <%= image_tag("logo.png") %>
      <p class="name">Scaled Agile Consulting</p>
    </div>
    <div class="title_group">
      <p class="title">Scaled Agile Consulting</p>
      <p class="sub_title">The Best Consultancy Provider based in Melbourne</p>
      <button class="submit_button">WORK WITH US</button>
    </div>
  </div>
  <div class="bright-arrow">
    <div class="a1" style="border-color: rgb(234, 236, 240);"></div>
    <div class="a2" style="border-color: rgb(234, 236, 240);"></div>
  </div>
</header>

<%= render 'shared/idea' %>
<%= render 'shared/services' %>
<%= render 'shared/advantages' %>
<%= render 'shared/consultants' %>

<div class="end" data-kui-anim="fadeIn" data-kui-event="bottom">
  <div class="container">
    <h1>Work with us</h1>
    <p>Want to know more? Or can not wait working with us?</p>
    <button class="submit_button">START</button>
  </div>
</div>
